{
  "type": "module",
  "name": "ai-saas-starter-kit",
  "version": "1.0.0",
  "description": "Production-ready AI SaaS starter kit with Next.js 16, React 19, TypeScript, and PostgreSQL",
  "author": "Karthikeyan N",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/karthiknitt/ai-saas-starter-kit.git"
  },
  "homepage": "https://github.com/karthiknitt/ai-saas-starter-kit#readme",
  "bugs": {
    "url": "https://github.com/karthiknitt/ai-saas-starter-kit/issues"
  },
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "saas",
    "starter-kit",
    "ai",
    "openai",
    "postgresql",
    "drizzle-orm",
    "tailwindcss",
    "better-auth"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:clean": "rm -rf .next && bun dev",
    "build": "next build",
    "build:analyze": "next build --turbopack && bunx vite-bundle-analyzer dist",
    "start": "next start",
    "lint": "biome check --write --css-parse-tailwind-directives=true",
    "format": "biome format --write --css-parse-tailwind-directives=true",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:setup": "bash scripts/setup-e2e-tests.sh",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "bun run scripts/seed.ts",
    "db:reset": "bun db:push && bun db:seed",
    "db:migrate:phase2": "bun run scripts/migrate-phase2.ts",
    "make-admin": "bun run scripts/make-admin.ts",
    "test:email": "bun run scripts/test-email.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "analyze": "bunx vite-bundle-analyzer .next/static",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.7",
    "@ai-sdk/react": "^3.0.21",
    "@arcjet/next": "^1.0.0-beta.16",
