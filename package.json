{
  "type": "module",
  "name": "starterkit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:clean": "rm -rf .next && pnpm dev",
    "build": "next build --turbopack",
    "build:analyze": "next build --turbopack && npx vite-bundle-analyzer dist",
    "start": "next start",
    "lint": "biome check --write --css-parse-tailwind-directives=true",
    "format": "biome format --write --css-parse-tailwind-directives=true",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "npx tsx scripts/seed.ts",
    "db:reset": "pnpm db:push && pnpm db:seed",
    "db:migrate:phase2": "npx tsx scripts/migrate-phase2.ts",
    "make-admin": "npx tsx scripts/make-admin.ts",
    "test:email": "npx tsx scripts/test-email.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "analyze": "npx vite-bundle-analyzer .next/static",
    "prepare": "lefthook install"
  },
